{
    "name": "aframe-stereo-material",
    "description": "use stereoscopic images or videos as material for your aframe objects",
    "version": "0.3.0",
    "license": "MIT",
    "scripts": {
        "build": "browserify examples/main.js -o examples/build.js",
        "dev": "budo examples/main.js:build.js --dir examples --port 8000 --live --open",
        "dist": "webpack browser.js dist/aframe-stereo-material.js && webpack -p browser.js dist/aframe-stereo-material.min.js"
    },
    "devDependencies": {
        "aframe": "^0.5.0",
        "browserify": "^12.0.1",
        "browserify-css": "^0.8.3",
        "budo": "^7.0.0",
        "ghpages": "0.0.3",
        "webpack": "^1.12.9",
        "aframe-sun-sky": "^3.0.3"
    },
    "keywords": [
        "aframe",
        "aframe-stereo-material",
        "aframe-scene",
        "webvr",
        "vr",
        "stereo",
        "stereoscopic"
    ],
    "repository": {
        "type" : "git",
        "url" : "https://github.com/cowonacid/aframe-stereo-material"
    }
}
